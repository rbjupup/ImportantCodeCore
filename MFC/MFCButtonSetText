    //设置按钮图表
    m_channengButton.SetImage(IDB_BITMAP1);
    m_channengButton.SetWindowText('\0');
    m_channengButton.SizeToContent();
    m_channengButton.SetTooltip(_T("产能界面"));


设置按钮图片，占据button全屏
要设置Cbutton属性为OwenerDraw
设置CBU为CBItmapbutton

m_compressButton.LoadBitmaps(IDB_BITMAP2,IDB_BITMAP1);
